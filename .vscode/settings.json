{
    "files.exclude": {
        "apps/": true,
        "**/.mypy_cache": true,
        "**/.idea": true,
        "**/.ruff_cache": true
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "compose.yaml": "compose.dev.yaml, compose.prod.yaml",
        ".pre-commit-config.yaml": ".env, .hadolint.yaml, .checkmake.ini, .opencommitignore",
    },
    "cSpell.words": [
        "golangci",
        "nodaemon",
        "protoc",
        "protos",
        "rcfile",
        "rpcinterface",
        "supervisorctl",
        "Supervisord"
    ]
}
