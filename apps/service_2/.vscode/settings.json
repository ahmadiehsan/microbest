{
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "files.exclude": {
        "**/.venv": true,
        "**/__pycache__": true,
        "**/.mypy_cache": true,
        "**/.idea": true,
        "**/.ruff_cache": true
    },
    "search.exclude": {
        "**/uv.lock": true
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "pyproject.toml": "uv.lock",
        "Dockerfile": ".dockerignore",
    },
    "cSpell.words": [
        "asgi",
        "checkmake",
        "configfile",
        "fastapi",
        "funcarg",
        "funcs",
        "grpcio",
        "gunicorn",
        "healthcheck",
        "httpx",
        "Instrumentor",
        "isort",
        "jaegertracing",
        "keepalive",
        "levelname",
        "logfile",
        "memlock",
        "mypy",
        "nodaemon",
        "protoc",
        "protos",
        "Pylint",
        "pyproject",
        "PYTHONPATH",
        "rpcinterface",
        "runserver",
        "shfmt",
        "supervisorctl",
        "supervisord",
        "tini",
        "ulimits",
        "uvicorn",
        "venv",
        "wsgi",
        "xpack"
    ]
}
